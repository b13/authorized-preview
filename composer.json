{
	"name": "b13/authorized-preview",
	"type": "typo3-cms-extension",
	"description": "Generate URLs to preview hidden languages without a backend login",
	"require": {
		"typo3/cms-core": "^9.5"
	},
	"replace": {
		"authorized_preview": "self.version"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "authorized_preview"
		}
	},
	"autoload": {
		"psr-4": {
			"B13\\AuthorizedPreview\\": "Classes/"
		}
	}
}
